/* Color Definitions */

$dark_grey: #2f2f2f;
$tan: #ffebc9;
$red: #8f2911;
$green: #468966;
$orange: #f3ae39;
$blue: #3AF;
$width: 75%;


/* Mixins */

@mixin rounded($radius: 10px) {
	border-radius: $radius;
	-moz-border-radius: $radius;
	-webkit-border-radius: $radius;
}

@mixin animate {
	$time: 0.3s;
	transition: background $time ease;
	-webkit-transition: background $time ease;
	-moz-transition: background $time ease;
	-o-transition: background $time ease;
}

/* Main */

body {
	background: $dark_grey;
	font-size: 18px;
	font-family: "Lucida Grande", "Lucida Sans Unicode", Verdana, sans-serif;
	color: $dark_grey;
}

header#masthead, section#content {
	background: $tan;
	padding: 20px;
}

#masthead {
	padding-top: 0;
}

#masthead>div, #content>div {
	width: $width;
	margin: 0 auto;
}

#content>div {
	@include rounded;
	background: #FFF;
	padding: 20px;
}

#masthead>div {
	margin-top: 0px;
}

#masthead h2 {
	background: $dark_grey;
	width: 99%;
	position: absolute;
	top: 0;
	left: 0;
	height: 20px;
	padding: 5px;
	text-align: right;
	color: #CCC;
	font-size: 0.9em;
	font-weight: normal;
	text-shadow: #000 0 0 1px;
}

#masthead h1 {
	width: 278px;
	height: 192px;
	text-indent: -999em;
	background: url(logo.gif) no-repeat;
}

nav ul {
	float: right;
	margin: -50px 0 0;
}

nav:after {
	content: ".";
	visibility: hidden;
	height: 1px;
	clear: both;
	display: block;
}

nav ul li {
	float: right;
	margin: 5px 0 0 15px;
}

nav ul li a {
	background: $red;
	padding: 10px 20px;
	@include rounded;
	text-decoration: none;
	color: #FFF;
	@include animate;
	
	&:hover {
		background-color: lighten($red, 10%);
	}
}

ul#archive {
	list-style: none;
}

ul#archive li {
	border-top: 1px dashed #BBB;
	padding: 5px;
}

ul#archive li time {
	float: right;
	text-align: left;
	font-size: 0.8em;
	color: #555;
}

#content a {
	&:link, &:visited {
		color: lighten($red, 10%);
	}
	
	&:hover {
		color: darken($red, 10%);
	}
}

#content h2>a, #content h2 {
		font-size: 40px;
		color: $red;
		letter-spacing: -2px;
		text-decoration: none;
}

body #content h2>a {
  &:link, &:visited {
		color: $red;
	}
	&:hover {
		color: lighten($red, 10%);
	}
}

#content h3 {
	font-size: 1.3em;
	background: $green;
	color: #FFF;
	font-weight: normal;
}

#content h4 {
	padding: 1em 0 0;
	border-bottom: 1px dashed #CDCDCD;
	font-size: 1.3em;
}

#content p {
	font-size: 1em;
	line-height: 1.5em;
	padding: 0.75em 0 0.75em;
}

p>code, div.post>pre {
	font-face: "Courier New", Courier, monospace;
	background: #EEE;
	border: 1px solid #DDD;
}

#content img {
	margin: 0 10px 0 0;
	padding: 5px;
	border: 2px solid #DDD;
	float: left;
}

#content img.math {
	display: block;
	float: none;
}

#disqus_thread .dsq-options {
	display: none;
}

#content img.maruku-png {
	float: none;
	display: inline;
	border-width: 0
}

em {
	font-style: italic;
}

ul {
	list-style: square;
	margin: 5px 0 5px 20px;
}

ul li {
	margin: 5px 0;
}

footer#foot {
	position: relative;
	width: $width;
	margin: 1em auto;
	
}

footer ul {
	list-style: none;
	margin: 0;
}

footer ul li {
	margin: 5px 0;
	padding: 5px;
	border-bottom: 1px dashed #555;
}

footer #recent-projects, #recent-posts, #friends
{
	width: 20%;
	float: left;
	font-size: 0.8em;
	padding: 10px;
}

footer #recent-posts
{
	width: 51%;
}

footer h3 {
	font-size: 1.4em;
	color: #FFF;
	text-shadow: #000 0 0 1px;
	border-bottom: 1px solid #555;
}

footer a
{
	text-decoration: none;
	&:link, &:visited {
		color: #CCC;
	}
	&:hover, &:active {
		color: #FFF;
		background: #222;
	}
}

/* PYGMENTS HIGHLIGHTS
   Thanks to Ken Collins */

	/* Pygment Highlight */

	$tmc: #fff;
	$tmc_b: #000;
	$tmc_k: rgb(231,125,60);
	$tmc_s: rgb(0,216,60);
	$tmc_se: rgb(76,76,76);
	$tmc_c: rgb(249,204,177);
